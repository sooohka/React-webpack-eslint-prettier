{
  "compilerOptions": {
    "baseUrl": "./src",
    "esModuleInterop": true,
    "jsx": "react-jsx",

    "lib": ["DOM", "ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5",
    "noEmit": true,
    "paths": {
      "@Components/*": ["components/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
